{
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/public/(.*)\\.(jpg|jpeg|png|gif|webp|svg|ico)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }